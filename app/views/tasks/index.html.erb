<h1>My Tasks</h1>

<ul>
  <% @tasks.each do |task| %>
    <li>
      <%= link_to task.title, task_path(task) %>
      <br>
      <%= link_to "Delete task", delete_task_path(task), method: :delete, data: {confirm: "Are you sure?"} %>
    </li>
    <% end %>
</ul>

<%= link_to "Create new task", new_task_path %>
